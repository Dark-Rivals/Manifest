{
    "manifest_version": 3,
  
    "name": " Auto Paste",
    "description": "This extension automatically paste data.",
   "sandbox": {
    "pages": [
      "background.js"
    ]
},
    "version": "1.0",
    "action": {
      "default_icon": "icon.png",
      "default_popup": "window.html"
    },
   "background": {
    "service_worker": "background.js"
},

    "icons": {
      "128": "icon.png",
      "48": "icon2.png"
    },

"permissions": [
 "tabs",
 "clipboardRead",
        "clipboardWrite",
        "storage"
 ],
    "host_permissions":[
      "https://docs.google.com/document/d/*"
    ]
  }
